<div class="review-box">
  <div class="review-info">
  <div class="review-star">
    <% for star in [1..@overview.review.rate] : %>
    <span class="fa fa-2x fa-star"></span>
    <% end %>
    <% for star in [1..5-@paper.review.rate] : %>
    <span class="fa fa-2x fa-star-o"></span>
    <% end %>
  </div>
    <div class="review-user">
      <p><%- @overview.review.user.name %></p>
      <img src="http://placehold.it/45x45">
    </div>
    <p><%- @overview.review.comment %></p>
  </div>
</div>
